<aiml>
	<category>
		<pattern>TEST $PRIORITY</pattern>
		<template>
			Priority Response
		</template>
	</category>
	<category>
		<pattern>TEST PRIORITY ZERO #</pattern>
		<template>
			0 or more response
		</template>
	</category>
	<category>
		<pattern>TEST PRIORITY _</pattern>
		<template>
			1 or more response
		</template>
	</category>
	<category>
		<pattern>TEST WORD MATCH</pattern>
		<template>
			word match response
		</template>
	</category>
	<category>
		<pattern>TEST ^ ZERO</pattern>
		<template>
			0 match response
		</template>
	</category>
	<category>
		<pattern>TEST ^ ZERO WORD</pattern>
		<template>
			0 match response
		</template>
	</category>
	<category>
		<pattern>TEST ZERO WORD</pattern>
		<template>
			0 match response
		</template>
	</category>
	<category>
		<pattern>TEST * * *</pattern>
		<template>
			1 match response <star/> <star/> <star/>
		</template>
	</category>
	<category>
		<pattern>DO YOU KNOW THE _ TO $SAN $JOSE</pattern>
		<template>
			<random>
				<li>Do you know the <star/> to san jose?</li>
				<li>Of course I know the <star/> to san jose!</li>
				<li>Are you stupid? I know the <star/> to everything!</li>
			</random>
		</template>
	</category>
	<category>
		<pattern>DO YOU KNOW WHAT THE ROCK IS COOKING</pattern>
		<template>
			It's 'Do you SMELL what the rock is cooking'.
		</template>
	</category>
</aiml>