<aiml>
	<topic name="TIME">
		<category>
			<pattern>HOW MUCH TIME *</pattern>
			<template>
				How much?
			</template>
		</category>
		<category>
			<pattern>4 *</pattern>
			<that>HOW MUCH TIME *</that>
			<template>
				<random>
					<li>That seems like a long time.</li>
					<li>Only four <star/> ? </li>
				</random>
			</template>
		</category>
		<category>
			<pattern>FOUR *</pattern>
			<that>HOW MUCH TIME *</that>
			<template>
				<srai>4 <star/></srai>
			</template>
		</category>
		<category>
			<pattern>*</pattern>
			<template> 
				<think><set name="topic">Time</set></think>
			</template>
		</category>
	</topic>
	<category>
		<pattern>HELLO</pattern>
		<template>
			<random>
				<li>Hey!</li>
				<li>Hello.</li>
				<li>Hi. How are you?</li>
			</random>
		</template>
	</category>
	<category>
		<pattern>HI</pattern>
		<template>
			<srai>HELLO</srai>
		</template>
	</category>
	<category>
		<pattern>DO YOU KNOW WHO * IS?</pattern>
		<template>
			<srai>WHO IS <star/></srai>
		</template>
	</category>
	<category>
		<pattern>I am *</pattern>
		<template>
			Hello <set name = "username"><star/></set>!
		</template>  
	</category>
	<category>
		<pattern>Good Night</pattern>
		<template>
			Goodbye <get name = "username"/> see you later!
		</template>  
	</category>
	<category>
		<pattern>DO YOU KNOW * TO *</pattern>
		<template>
			Do you know <star/> to <star/>?
		</template>
	</category>
</aiml>