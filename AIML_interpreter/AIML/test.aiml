<aiml>
	<category>
		<pattern>TEST $PRIORITY</pattern>
		<template>
			Priority Response
		</template>
	</category>
	<category>
		<pattern>TEST PRIORITY ZERO #</pattern>
		<template>
			0 or more response
		</template>
	</category>
	<category>
		<pattern>TEST PRIORITY _</pattern>
		<template>
			1 or more response
		</template>
	</category>
	<category>
		<pattern>TEST WORD MATCH</pattern>
		<template>
			word match response
		</template>
	</category>
	<category>
		<pattern>TEST ^ ZERO</pattern>
		<template>
			0 match response
		</template>
	</category>
	<category>
		<pattern>TEST ^ ZERO WORD</pattern>
		<template>
			0 match response
		</template>
	</category>
	<category>
		<pattern>TEST ZERO WORD</pattern>
		<template>
			0 match response
		</template>
	</category>
	<category>
		<pattern>TEST *</pattern>
		<template>
			1 match response
		</template>
	</category>
	<category>
		<pattern>DO YOU KNOW THE _ TO $SAN $JOSE</pattern>
		<template>
			Do you know the way to san jose?
		</template>
	</category>
	<category>
		<pattern>DO YOU KNOW WHAT THE ROCK IS COOKING</pattern>
		<template>
			It's 'Do you SMELL what the rock is cooking'.
		</template>
	</category>
</aiml>