<aiml>
	<category>
		<pattern>TEST SET</pattern>
		<template>
			Setting "<set name="TestVar">Some Value</set>".
		</template>
	</category>
	<category>
		<pattern>TEST SET *</pattern>
		<template>
			Setting "<set name="TestVar"><star/></set>".
		</template>
	</category>
	<category>
		<pattern>TEST GET</pattern>
		<template>
			Getting "<get name="TestVar"/>".
		</template>
	</category>
	<category>
		<pattern>TEST ^ ^ ^ ^ ^ ^ ^ ^</pattern>
		<template>
			Test response <star index="1"/> <star index="2"/> <star index="3"/> <star index="4"/> <star index="5"/> <star index="6"/> <star index="7"/> <star index="8"/>
		</template>
	</category>
	<category>
		<pattern>DO YOU KNOW THE _ TO $SAN $JOSE</pattern>
		<template>
			<random>
				<li>Do you know the <star/> to san jose?</li>
				<li>Of course I know the <star/> to san jose!</li>
				<li>Are you stupid? I know the <star/> to everything!</li>
			</random>
		</template>
	</category>
</aiml>